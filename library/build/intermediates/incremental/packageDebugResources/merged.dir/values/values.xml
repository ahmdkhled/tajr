<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MovingButton">
        <attr name="mb_move_direction">
            <enum name="all" value="0"/>
            <enum name="horizontal_vertical" value="1"/>
            <enum name="horizontal" value="2"/>
            <enum name="vertical" value="3"/>
            <enum name="still" value="4"/>
        </attr>
        <attr format="dimension" name="mb_movement"/>
        <attr format="dimension" name="mb_movement_left"/>
        <attr format="dimension" name="mb_movement_right"/>
        <attr format="dimension" name="mb_movement_top"/>
        <attr format="dimension" name="mb_movement_bottom"/>
        <attr format="dimension" name="mb_offset_inner"/>
        <attr format="dimension" name="mb_offset_outer"/>
        <attr format="integer" name="mb_vibration_duration"/>
        <attr format="integer" name="mb_event_volume"/>
    </declare-styleable>
    <dimen name="default_offset_inner">16dp</dimen>
    <dimen name="default_offset_outer">23dp</dimen>
</resources>